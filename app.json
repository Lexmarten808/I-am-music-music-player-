{
  "expo": {
    "name": "I am music",
    "slug": "i-am-music",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.brandon_lasprilla.iammusic"
    },
    "web": {
      "plugins": [
        "expo-asset"
      ]
    },
    "plugins": [
      "expo-audio",
      "expo-video",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "d50373af-381c-48d7-bc91-d70bd2855b0e"
      }
    },
    "owner": "brandonlasprilla"
  }
}
